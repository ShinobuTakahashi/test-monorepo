{
  "name": "@shinobu_takahashi/test-cli-extra",
  "version": "1.0.2",
  "description": "test-extra",
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
